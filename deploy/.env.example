# Alibaba Cloud FC Environment Variables
# Copy this file to .env and fill in your values

# Aliyun Account
ALIBABA_CLOUD_ACCESS_KEY_ID=your_access_key_id
ALIBABA_CLOUD_ACCESS_KEY_SECRET=your_access_key_secret

# Deployment Region (choose the closest to you)
# cn-hangzhou - 杭州 (华东1) - 推荐
# cn-shanghai - 上海 (华东2)
# cn-beijing - 北京 (华北2)
# cn-shenzhen - 深圳 (华南1)
# cn-chengdu - 成都 (西南1)
REGION=cn-hangzhou

# Function Configuration
SERVICE_NAME=sales-agent-service
FUNCTION_NAME=sales-agent-api
MEMORY_SIZE=512
TIMEOUT=60

# Data Configuration
DATA_DIR=/tmp
